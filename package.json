{
  "name": "react-arbiter",
  "version": "0.2.0",
  "description": "Recall all your modules to extend your SPA dynamically at runtime.",
  "main": "dist/index.js",
  "scripts": {
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "build": "tsc --project tsconfig.json",
    "test:unit": "jest src --passWithNoTests",
    "test:watch": "npm run test:unit -- --watch",
    "test": "npm run lint && npm run test:unit",
    "prettier": "prettier --config prettier.config.js --write \"src/**/*.{ts,tsx}\"",
    "prepush": "npm run prettier && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FlorianRappl/react-arbiter.git"
  },
  "keywords": [
    "react",
    "arbiter",
    "portal",
    "modules",
    "load",
    "spa"
  ],
  "author": "Florian Rappl",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FlorianRappl/react-arbiter/issues"
  },
  "homepage": "https://github.com/FlorianRappl/react-arbiter#readme",
  "devDependencies": {
    "@types/enzyme": "^3.1.15",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/jest": "^23.3.12",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "enzyme-to-json": "^3.3.5",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "prettier": "^1.15.3",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.2.2"
  },
  "peerDependencies": {
    "react": "16.x || 17.x"
  }
}
