{
  "name": "react-arbiter",
  "version": "0.4.1",
  "description": "Recall all your modules to extend your SPA dynamically at runtime.",
  "main": "dist/index.js",
  "scripts": {
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "build": "tsc --project tsconfig.json",
    "test:unit": "jest src --passWithNoTests",
    "test:watch": "npm run test:unit -- --watch",
    "test": "npm run lint && npm run test:unit",
    "prettier": "prettier --config prettier.config.js --write \"src/**/*.{ts,tsx}\"",
    "prepush": "npm run prettier && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FlorianRappl/react-arbiter.git"
  },
  "keywords": [
    "react",
    "arbiter",
    "portal",
    "modules",
    "load",
    "spa"
  ],
  "author": "Florian Rappl",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FlorianRappl/react-arbiter/issues"
  },
  "homepage": "https://github.com/FlorianRappl/react-arbiter#readme",
  "devDependencies": {
    "@types/enzyme": "^3.1.17",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.3",
    "@types/react": "^16.8.2",
    "@types/react-dom": "^16.8.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "enzyme-to-json": "^3.3.5",
    "jest": "^24.1.0",
    "jest-cli": "^24.1.0",
    "prettier": "^1.16.4",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "ts-jest": "^24.0.0",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.3.3"
  },
  "peerDependencies": {
    "react": "16.x"
  }
}
